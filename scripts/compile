source "$RootDir/scripts/lib/colors.sh";
source "$RootDir/scripts/lib/npm_exec.sh"

printf "${BIYellow}compiling${Color_Off}\n"

rm $RootDir/compiled/main.js;
npm_exec babel $RootDir/main.js --out-file $RootDir/compiled/main.js;

rm $RootDir/compiled/app/routes.js;
npm_exec babel $RootDir/app/routes.js --out-file $RootDir/compiled/app/routes.js;

printf "${BGreen}finished compiling${Color_Off}\n"
